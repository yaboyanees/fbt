<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
  
<div id="formDevise">
        <legend>Please Enter Your Details</legend>
<fieldset>
  <p><%= f.label :email, 'USMA Email' %><br />
  <%= f.text_field :email, :size=>'28' %></p>

  <p><%= f.label :first_name, 'First Name' %><br />
  <%= f.text_field :first_name, :size=>'15' %></p>
 
  <p><%= f.label :last_name, 'Last Name' %><br />
  <%= f.text_field :last_name, :size=>'15' %></p>
 
  <p><%= f.label :password,'Password'  %><br />
  <%= f.password_field :password, :size=>'15' %></p>
 
  <p><%= f.label :password_confirmation, 'Confirm Password'  %><br />
  <%= f.password_field :password_confirmation, :size=>'15' %></p> 
  <div id="submit">
  <p><%= f.submit "Register" %></p>
  </div>
     </fieldset>
</div> 
<% end %>