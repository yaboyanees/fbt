<title>Login</title>
<%= form_for(resource, :as => resource_name, :url => new_session_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
  
<h2></h2>
<div id="formDevise">
  <legend>Please Enter Your Details</legend>
  <fieldset>

  <p><%= f.label :email, 'USMA Email' %><br />
  <%= f.text_field :email %></p>



  <p><%= f.label :password, 'Password' %><br />
  <%= f.password_field :password, :size =>'15' %></p>



  <div id="submit">
  <p><%= f.submit "Login" %></p>
      </div>
         <a><%= render "links" %></a>


    </fieldset>

     </div>
<% end %>